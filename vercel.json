{
    "version": 2,
    "builds": [
      {
        "src": "api/scrape.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/scrape",
        "dest": "api/scrape.js"
      },
      {
        "src": "/(styles\\.css|script\\.js)",
        "dest": "/public/$1",
        "headers": {
          "Cache-Control": "public, max-age=31536000, immutable"
        }
      },
      {
        "src": "/(.*)",
        "dest": "/public/index.html"
      }
    ]
  }