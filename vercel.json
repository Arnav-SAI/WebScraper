{
    "version": 2,
    "builds": [
      {
        "src": "api/scrape.js",
        "use": "@vercel/node"
      },
      {
        "src": "static/**",
        "use": "@vercel/static"
      },
      {
        "src": "index.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/scrape",
        "dest": "api/scrape.js"
      },
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }