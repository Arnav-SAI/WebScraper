{
    "version": 2,
    "builds": [
      {
        "src": "wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.11",
          "timeout": 10
        }
      },
      {
        "src": "static/**",
        "use": "@vercel/static"
      },
      {
        "src": "templates/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "wsgi.py"
      }
    ],
    "env": {
      "PYTHONUNBUFFERED": "1"
    }
  }